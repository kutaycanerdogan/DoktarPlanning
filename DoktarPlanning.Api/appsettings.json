{
  "AllowedOrigins": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Seq" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=DoktarPlanning;Integrated Security=True;TrustServerCertificate=True"
  },
  "Hangfire": { "UseHangfire": true },
  "Redis": {
    "Configuration": "localhost:6379",
    "InstanceName": "doktar:"
  },
  "Jwt": {
    "Key": "Key",
    "Issuer": "Issuer",
    "Audience": "Audience"
  },
  "Smtp": {
    "Host": "smtp.doktar.com",
    "Port": "587",
    "User": "smtp-user",
    "Password": "password",
    "From": "noreply@doktar.com"
  },
  "KeyVault": {
    "VaultUri": ""
  }
}